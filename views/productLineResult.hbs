<!DOCTYPE html>
<html>
<head>
    <title>Matching Products</title>
    <style>
    table {
      font-family: Arial, sans-serif;
      border-collapse: collapse;
      width: 100%;
    }

    td, th {
      border: 1px solid #dddddd;
      text-align: left;
      padding: 8px;
    }

    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
    <h1>Matching Products</h1>
    <table>
    <thead>
      <tr>
        <th>Product Name</th>
        {{!-- <th>Image</th> --}}
        <th>Branch</th>
        <th>City</th>
      </tr>
    </thead>
    <tbody>
    {{#each matchingProducts}}
        <div>
            <tr>
                <td>{{this.name}}</td>
                {{!-- <td>{{this.image}}</td> --}}
                <td>{{this.Branch}}</td>
                <td>{{this.City}}</td>
            </tr>
        </div>
    {{/each}}
    </tbody>
    </table>
</body>
</html>
